{
  "check executable exist": {
    "prefix": "te",
    "body": "type ${1} >/dev/null 2>&1"
  },
  "cat eof": {
    "prefix": "ce",
    "body": [
      "cat <<EOF",
      "${1}",
      "EOF"
    ]
  },
  "cat eof": {
    "prefix": "cq",
    "body": [
      "cat <<'EOF'",
      "${1}",
      "EOF"
    ]
  }
}
